package com.great.base.entity;

import net.sf.json.JSONObject;

public class Message2Page {

	private boolean success;
	private String code;
	private String msg;
	private JSONObject job;

	public Message2Page(boolean success, String code, String msg, JSONObject job) {
		super();
		this.success = success;
		this.code = code;
		this.msg = msg;
		this.job = job;
	}

	public Message2Page(boolean success, String code, String msg) {
		super();
		this.success = success;
		this.code = code;
		this.msg = msg;
	}

	public boolean isSuccess() {
		return success;
	}

	public void setSuccess(boolean success) {
		this.success = success;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getMsg() {
		return msg;
	}

	public void setMsg(String msg) {
		this.msg = msg;
	}

	public JSONObject getJob() {
		return job;
	}

	public void setJob(JSONObject job) {
		this.job = job;
	}

}
